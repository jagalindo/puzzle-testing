<dependency from="(line)" to="(line)" weight="90" />
<dependency from="(line)" to="(dpl)" weight="21" />
<dependency from="(rect AND !color)" to="(rect)" weight="12" />
<dependency from="(rect AND !color)" to="(rect AND !color)" weight="7" />
<dependency from="(dpl)" to="(dpl)" weight="180" />
<dependency from="(!color AND line)" to="(line)" weight="8" />
<dependency from="(!color AND line)" to="(!color AND line)" weight="4" />
<dependency from="(wipe)" to="(dpl)" weight="10" />
<dependency from="(wipe)" to="(wipe)" weight="12" />
<dependency from="(rect AND color)" to="(rect)" weight="3" />
<dependency from="(rect AND color)" to="(rect AND color)" weight="3" />
<dependency from="(rect AND color AND !fill)" to="(rect)" weight="11" />
<dependency from="(rect AND color AND !fill)" to="(color)" weight="1" />
<dependency from="(rect AND color AND !fill)" to="(rect AND color)" weight="1" />
<dependency from="(rect AND color AND !fill)" to="(rect AND color AND !fill)" weight="10" />
<dependency from="(fill)" to="(rect)" weight="23" />
<dependency from="(fill)" to="(color)" weight="3" />

<?xml version="1.0" encoding="UTF-8"?>
<feature-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.tdg-seville.info/benavides/featuremodelling/feature-model.xsd">

<feature name="doc_gen">

<binaryRelation name="rel1">
  <cardinality max="1" min="1"/>
  <solitaryFeature name="analysis">
     
    <binaryRelation name="rel4">
  		<cardinality max="1" min="1"/>
  		<solitaryFeature name="language_analysis">
  			<setRelation name="rel7">
				<cardinality max="5" min="1"/>
				<groupedFeature name="cobol">
					<setRelation name="rel8">
						<cardinality max="1" min="1"/>
						<groupedFeature name="ibm_cobol"/>
						<groupedFeature name="microfocus_cobol"/>
					</setRelation>
				</groupedFeature>
				<groupedFeature name="jcl"/>
				<groupedFeature name="sql"/>
				<groupedFeature name="delphi"/>
				<groupedFeature name="progress"/>
   			</setRelation>
  		</solitaryFeature>
	</binaryRelation>

    <binaryRelation name="rel5">
  		<cardinality max="1" min="0"/>
  		<solitaryFeature name="version_mngt"/>
	</binaryRelation>     
	
   <binaryRelation name="rel6">
  		<cardinality max="1" min="0"/>
  		<solitaryFeature name="subsystems"/>
	</binaryRelation>     
     
  </solitaryFeature>
</binaryRelation>



<binaryRelation name="rel2">
  <cardinality max="1" min="1"/>
  <solitaryFeature name="presentation">
  
  	<binaryRelation name="rel10">
  		<cardinality max="1" min="1"/>
  		<solitaryFeature name="localization">
			<setRelation name="rel14">
				<cardinality max="2" min="1"/>
				<groupedFeature name="english"/>
				<groupedFeature name="dutch"/>
			</setRelation>  		
  		</solitaryFeature>
	</binaryRelation>     

  	<binaryRelation name="rel11">
  		<cardinality max="1" min="1"/>
  		<solitaryFeature name="interaction">
			<setRelation name="rel15">
				<cardinality max="1" min="1"/>
				<groupedFeature name="static"/>
				<groupedFeature name="dynamic"/>
			</setRelation>  		
  		</solitaryFeature>  		
	</binaryRelation>   
  
  	<binaryRelation name="rel12">
  		<cardinality max="1" min="1"/>
  		<solitaryFeature name="main_pages">
			<setRelation name="rel16">
				<cardinality max="8" min="1"/>
				<groupedFeature name="program_page">
					<setRelation name="rel18">
					<cardinality max="4" min="1"/>
						<groupedFeature name="annotation_section"/>
						<groupedFeature name="activation_section"/>
						<groupedFeature name="entities_section"/>
						<groupedFeature name="parameters_section"/>
					</setRelation>  		
  				</groupedFeature>  		 
				
				<groupedFeature name="copybook_page"/>
				<groupedFeature name="statistics_page">
					<setRelation name="rel19">
					<cardinality max="1" min="1"/>
						<groupedFeature name="with_history"/>
						<groupedFeature name="without_history"/>
					</setRelation>  		
  				</groupedFeature> 	
	
				<groupedFeature name="indexes"/>
				<groupedFeature name="search_page"/>
				<groupedFeature name="subsystem_page"/>
				<groupedFeature name="source_page"/>
				<groupedFeature name="source_difference"/>												
			</setRelation>  		
  		</solitaryFeature>  		  		
	</binaryRelation>   

  	<binaryRelation name="rel13">
  		<cardinality max="1" min="0"/>
  		<solitaryFeature name="visualizations">
			<setRelation name="rel17">
				<cardinality max="5" min="1"/>
				<groupedFeature name="perform_graph"/>
				<groupedFeature name="conditional_perform_graph"/>
				<groupedFeature name="jcl_graph"/>
				<groupedFeature name="subsystem_graph"/>
				<groupedFeature name="overview_graph"/>		
			</setRelation>  		
  		</solitaryFeature>  		   		
	</binaryRelation>   
	  	
  </solitaryFeature>
</binaryRelation>


<binaryRelation name="rel3">
  <cardinality max="1" min="1"/>
  <solitaryFeature name="database">
  	<setRelation name="rel9">
		<cardinality max="1" min="1"/>
			<groupedFeature name="db2"/>
			<groupedFeature name="oracle"/>
			<groupedFeature name="mysql"/>
	</setRelation>
  </solitaryFeature>
</binaryRelation>


</feature>

<requires feature="subsystem_page" name="c1" requires="subsystems"/>
<requires feature="subsystem_graph" name="c2" requires="subsystems"/>
<requires feature="source_difference" name="c3" requires="version_mngt"/>
<requires feature="perform_graph" name="c4" requires="cobol"/>
<requires feature="conditional_perform_graph" name="c5" requires="cobol"/>
<requires feature="jcl_graph" name="c6" requires="jcl"/>
<excludes feature="static" name="c7" excludes="annotation_section"/>
<excludes feature="static" name="c8" excludes="search_page"/>
 
</feature-model>
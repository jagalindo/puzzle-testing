<?xml version="1.0" encoding="UTF-8"?>
<feature-model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.tdg-seville.info/benavides/featuremodelling/feature-model.xsd">
<feature name="Aircraft">
	<binaryRelation name="0">
	<cardinality max="1" min="1"/>
	<solitaryFeature name="Wing">
		<setRelation name="1">
		<cardinality max="3" min="1"/>
		<groupedFeature name="High">
		</groupedFeature>
		<groupedFeature name="Shoulder">
		</groupedFeature>
		<groupedFeature name="Low">
		</groupedFeature>
		</setRelation>

	</solitaryFeature>

	</binaryRelation>

	<binaryRelation name="2">
	<cardinality max="1" min="0"/>
	<solitaryFeature name="Engine">
		<setRelation name="3">
		<cardinality max="1" min="1"/>
		<groupedFeature name="Jet">
		</groupedFeature>
		<groupedFeature name="Piston">
		</groupedFeature>
		</setRelation>

	</solitaryFeature>

	</binaryRelation>

	<binaryRelation name="4">
	<cardinality max="1" min="1"/>
	<solitaryFeature name="Materials">
		<setRelation name="5">
		<cardinality max="4" min="1"/>
		<groupedFeature name="Metal">
		</groupedFeature>
		<groupedFeature name="Wood">
		</groupedFeature>
		<groupedFeature name="Cloth">
		</groupedFeature>
		<groupedFeature name="Plastic">
		</groupedFeature>
		</setRelation>

	</solitaryFeature>

	</binaryRelation>

</feature>
</feature-model>
